<h1>Listing dexes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @dexes.each do |dex| %>
      <tr>
        <td><%= dex.description %></td>
        <td><%= link_to 'Show', dex %></td>
        <td><%= link_to 'Edit', edit_dex_path(dex) %></td>
        <td><%= link_to 'Destroy', dex, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dex', new_dex_path %>
